namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int a;
            Console.WriteLine("Введите число от 1 до 10");
            a = Convert.ToInt32(Console.ReadLine());
            if (a >= 3)
                Console.WriteLine("Зачет");
            else if (a < 3)
                Console.WriteLine("Незачет");
            Console.ReadKey ();
        }
    }
}
